<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net5.0-windows</TargetFramework>
	<LangVersion>9.0</LangVersion>
    <UseWPF>true</UseWPF>
    <AssemblyName>CoolWindow</AssemblyName>
    <ApplicationIcon>Dialog.ico</ApplicationIcon>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="Resource\Dialog.ico" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Prism.Unity" Version="8.1.97" />
    <PackageReference Include="ReactiveProperty.WPF" Version="7.12.0" />
  </ItemGroup>
  <ItemGroup>
    <Resource Include="Resource\Dialog.ico">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
  </ItemGroup>
</Project>